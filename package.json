  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "scripts": {
    "dev": "rollup -c scripts/rollup.config.ts --configPlugin=typescript={tsconfig:'scripts/tsconfig.build.json'} --bundleConfigAsCjs --watch --environment NODE_ENV:dev",
